#!/usr/bin/env bash

UnidadH="$HOME/UnidadH"

if [ -f "$UnidadH/eps-scripts/gnome-terminal.custom.xml" ]; then
	gconftool-2 --load="$UnidadH/eps-scripts/gnome-terminal.custom.xml"
fi

cd "$( dirname "${BASH_SOURCE[0]}" )"

set +e

for folder in $(ls -d */); do 

	if [ -e $folder${folder%%/}.bash ]; then 
		./$(echo -n $folder${folder%%/}.bash) || true
	fi

done
